@model ProjectOne.Models.RegisterModel

<h2>Регистрация</h2>

<form asp-action="Register" asp-controller="Account" asp-anti-forgery="true">
    <div class="validation" asp-validation-summary="All"></div>
    <div>
        <div>
            <label asp-for="Email">Введите Email</label><br />
            <input type="text" asp-for="Email" />
        </div>
        <div>
            <label asp-for="Password">Введите пароль</label><br />
            <input asp-for="Password" />
        </div>
        <div>
            <label asp-for="ConfirmPassword">Повторите пароль</label><br />
            <input asp-for="ConfirmPassword" />
        </div>
        <div>
            <label asp-for="Name">Введите Имя</label><br />
            <input type="text" asp-for="Name" />
        </div>
        <div>
            <label asp-for="SureName">Введите Фамилию</label><br />
            <input type="text" asp-for="SureName" />
        </div>
        <div>
            <label asp-for="Serie">Введите серию</label><br />
            <input type="number" asp-for="Serie" />
        </div>
        <div>
            <label asp-for="Number">Введите номер</label><br />
            <input type="number" asp-for="Number" />
        </div>
        <div>
            <label asp-for="Place">Введите место выдачи</label><br />
            <input type="text" asp-for="Place" />
        </div>
        <div>
            <input type="submit" value="Регистрация" />
        </div>
    </div>
</form>

